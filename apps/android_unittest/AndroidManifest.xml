<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.structures.${target}"
    android:versionCode="1"
    android:versionName="1.0">
    <uses-sdk android:minSdkVersion="${ANDROID_SDK_VERSION}" android:targetSdkVersion="${ANDROID_SDK_VERSION}"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-feature android:glEsVersion="0x00020000"></uses-feature>"
    <application android:label="${target}" android:debuggable="true">
        <!--<uses-library android:name="android.test.runner" />-->
        <activity android:name="com.structures.${target}.ActivityStart"
            android:label="${target}"
            android:screenOrientation="fullSensor"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:debuggable="${debuggable}">
            <meta-data android:name="android.app.lib_name" android:value="${target}"/>
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
    <instrumentation android:name="com.structures.${target}.TestRunner"
        android:targetPackage="com.structures.${target}"
        android:label="Structures Unittests">
    </instrumentation>
</manifest>
